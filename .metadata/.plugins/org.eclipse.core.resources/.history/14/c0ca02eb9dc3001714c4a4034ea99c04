package testData;

import utility.DateUtil;

public class MyFamilyData {
	String DepDOB;
	String DepRelation;
	String TobaccoUse;
	
	public String getDepDateOfBirth() {
		return DepDOB;
	}

	public void setDepDateOfBirth(String dateOfBirth, int age) {
		if (dateOfBirth.equals("Dynamic")) {
			DepDOB = DateUtil.randomDate(age);
		} 
		else 
			DepDOB = dateOfBirth;
	}
	
	public void setDepDateOfBirth(String dateOfBirth) {
		
		DepDOB = dateOfBirth;
	}
	
	public String getDependentRelation() {
		return DepRelation;
	}

	public void setDependentRelation(String dependentRelation) {
		
		DepRelation=dependentRelation;	
	}
	public String getTobaccoUse() {
		return TobaccoUse;
	}

	public void setTobaccoUse(String tobaccouse) {
		
		TobaccoUse=tobaccouse;	
	}
	
}
