package testData;

import utility.StringUtil;

public class AdditionalEmailData {
	String AdditionalEMail;
	
	public void setAdditionalEMail(String strAdditionalEmail)
	{
		if (strAdditionalEmail.equals("Dynamic"))
			AdditionalEMail = StringUtil.generateRandomString(5).toLowerCase() + "@test.com";
		else
			AdditionalEMail=strAdditionalEmail;
	}
	
	public String getAdditionalEMail()
	{
		return AdditionalEMail;
	}
}
